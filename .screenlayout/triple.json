{
	"DP-2-2": {
		"_comment": "left",
		"_exec": [
			"bspc monitor {monitor} -g {cfg[mode]}+0+0",
			"for dsk in `bspc query -D`; do bspc desktop $dsk -m {monitor}; done",
			"bspc monitor {monitor} -o I III IV;"
		],
		"_flags": [],

		"output": "DP-2-2",
		"mode": "1920x1080",
		"pos": "0x0",
		"rotate": "left"
	},

	"DP-1-3": {
		"_comment": "middle",
		"_exec": [
			"bspc monitor {monitor} -g {cfg[mode]}+0+0",
			"for dsk in `bspc query -D`; do bspc desktop $dsk -m {monitor}; done",
			"bspc monitor {monitor} -o II V VI VII VIII;"
		],
		"_flags": [
			"--primary"
		],

		"output": "DP-1-3",
		"mode": "1920x1080",
		"pos": "1080x840",
		"rotate": "normal"
	},

	"DP-1-2": {
		"_comment": "right",
		"_exec": [
			"bspc monitor {monitor} -g {cfg[mode]}+0+0",
			"for dsk in `bspc query -D`; do bspc desktop $dsk -m {monitor}; done",
			"bspc monitor {monitor} -o IX X;"
		],
		"_flags": [],

		"output": "DP-1-2",
		"mode": "1920x1080",
		"pos": "3000x840",
		"rotate": "normal"
	}
}
